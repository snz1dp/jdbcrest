{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Launch SQLServiceRegistryImpl",
      "request": "launch",
      "mainClass": "com.snz1.jdbc.rest.service.impl.SQLServiceRegistryImpl",
      "projectName": "dashboard"
    },
    {
      "type": "java",
      "name": "Launch Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "Launch Application",
      "request": "launch",
      "mainClass": "com.snz1.jdbc.rest.Application",
      "projectName": "dashboard",
      "env": {
        "TABLE_AUTHORIZE": "file://${workspaceFolder}/src/test/resources/table-authorize.yaml",
        "SERVICE_AUTHORIZE": "file://${workspaceFolder}/src/test/resources/service-authorize.yaml",
        "PERMISSION_DEFINITION": "file://${workspaceFolder}/src/test/resources/permission-definition.yaml",
        "PERMISSION_ENABLED": "true",
        "SSO_ENABLED": "true",
        "SSO_SIMULATE": "true",
        "CONFIG_TYPE": "cluster",
        "SQL_LOCATION": "file://${workspaceFolder}/src/test/resources/sql/services"
      }
    }
  ]
}
